<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>

    <link rel="stylesheet"
          href="https://fonts.googleapis.com/css?family=Open+Sans">

    <link rel="stylesheet"
          href="/assets/bootstrap/dist/css/bootstrap.min.css">

    <link rel="stylesheet"
          href="/assets/bootstrap-horizon/bootstrap-horizon.css">

    <link rel='stylesheet'
          href='/stylesheets/style.css'/>
</head>
<body>
<div class="container-fluid">
    <header class="row kiosk-header">
        <div class="col-md-12 text-center">
            <img src="/images/butterfish-horiz.png"
                 class="pull-left"
                 alt="Butterfish">

            <span class="orders-waiting">
                <span class="current-order-index">0</span> /
                <span class="num-orders">0</span>
            </span>

            <div class="date-time pull-right"></div>
        </div>
    </header>

    <header class="row order-header">
        <div class="col-md-12 order-header-inner">&nbsp;</div>
    </header>

    <div class="row row-horizon order-columns"></div>
</div>

<script>
    window.SOCKETPATH = '<%= path %>';
    window.locationId = '<%= locationId %>';
</script>

<script src="/assets/moment/min/moment.min.js"></script>
<script src="/assets/jquery/dist/jquery.min.js"></script>
<script src="/assets/lodash/dist/lodash.min.js"></script>
<script src="/socket.io/socket.io.js"></script>
<script src="/assets/mousetrap/mousetrap.min.js"></script>

<script src="/js/orders.js"></script>
<script src="/js/header.js"></script>
<script src="/js/app.js"></script>
<script src="/js/keyboard.js"></script>
<script src="/js/StarWebPrintBuilder.js"></script>
<script src="/js/StarWebPrintTrader.js"></script>

</script>
<script src="/js/print.js"></script>

</body>
</html>
